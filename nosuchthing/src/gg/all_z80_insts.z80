  sbc hl, bc
  ld [$DE34], bc
  neg
  retn
  im 0
  ld i, a
  in c, [c]
  out [c], c
  adc hl, bc
  ld bc, [$DEB4]
  reti
  ld r, a

  ;$ED50
  in d, [c]
  out [c], d
  sbc hl, de
  ld [$DE35], de
  im 1
  ld a, i
  in e, [c]
  out [c], e
  adc hl, de
  ld de, [$DEB5]
  im 2
  ld a, r

  ; $ED60
  in h, [c]
  out [c], h
  sbc hl, hl
  rrd
  in l, [c]
  out [c], l
  adc hl, hl
  rld
  
  ; $ED70
  sbc hl, sp
  ld [$DE37], sp
  in a, [c]
  out [c], a
  adc hl, sp
  ld sp, [$DEB7]

  ; $EDA0
  ldi
  cpi
  ini
  outi
  ldd
  cpd
  ind
  outd

  ; $EDB0
